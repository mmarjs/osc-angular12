  "scripts": {
    "ng": "nx",
    "start": "./node_modules/.bin/ng serve",
    "start:local": "./node_modules/.bin/ng serve --configuration local",
    "build": "./node_modules/.bin/ng build",
    "build:stats": "./node_modules/.bin/ng build --stats-json",
    "sentry": "./node_modules/.bin/sentry-cli",
    "lint": "./node_modules/.bin/nx workspace-lint && ./node_modules/.bin/ng lint",
    "lint:api": "./node_modules/.bin/tslint -p libs/api/tsconfig.json -c libs/api/tslint.json -e **/snapshots/*.json --fix",
    "test": "jest --coverage",
    "e2e": "./node_modules/.bin/ng e2e",
    "backend": "yarn schematic backend --fetch && yarn lint:api",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
